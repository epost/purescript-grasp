{
  "name": "purescript-grasp",
  "version": "1.0.0",
  "description": "PureScript Graph Specification Processor",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "PATH=node_modules/.bin:$PATH node_modules/.bin/pulp run",
    "build": "PATH=node_modules/.bin:$PATH node_modules/.bin/pulp build",
    "test": "PATH=node_modules/.bin:$PATH node_modules/.bin/pulp test",
    "ex": "PATH=node_modules/.bin:$PATH make example/test1.png",
    "bundle": "PATH=node_modules/.bin:$PATH browserify browser/grasp.js > dist/grasp.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epost/purescript-grasp.git"
  },
  "keywords": [
    "purescript",
    "graph",
    "parser"
  ],
  "author": "Erik Post",
  "license": "ISC",
  "dependencies": {
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "bower": "^1.8.8",
    "pulp": "11.0.2",
    "purescript": "^0.12.1"
  }
}
